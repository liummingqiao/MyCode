
/* 定义一个容器 */
.container{
  width: 100%;
  height: 1200rpx;
  background-color: #f5f5f5;
  display: flex;
  flex-direction: column;
}
/* 包包大冒险和其他大冒险 */
.son{
  margin:0 auto;
  width:700rpx;
  height: 200rpx;
  display: flex;
  flex-direction: row;
  border: 1px solid #e5e5e5;
  margin-top: 30rpx;
  border-radius: 3%;
  background-color: #ffffff;
}
/* 包包大冒险和其他大冒险文本一 */
.text{
  display: flex;
  margin:10px 5px 15px 25px;
  flex-direction: column;
}
/* 包包大冒险和其他大冒险文本二 */
.text2{
  font-size: 50rpx;
  font-weight: 350;
}
/* 包包大冒险和其他大冒险文本三 */
.text3{
  margin-top: 26rpx;
  font-weight: 100;
  font-size: 37rpx;
  color: gray;
}
/* logo图标属性设置 */
.logo{
  width: 150rpx;
  height: 150rpx;
  margin:20rpx 0rpx 15rpx 40rpx;  /* 上右下左 */
}